{
  "name": "blueberry-pancake",
  "version": "1.0.0",
  "description": "Blind tasting event management application",
  "private": true,
  "workspaces": [
    "backend",
    "frontend"
  ],
  "scripts": {
    "dev:backend": "cd backend && npm run dev",
    "dev:frontend": "cd frontend && npm run dev",
    "dev": "npm run dev:backend & npm run dev:frontend",
    "install:all": "npm install && cd backend && npm install && cd ../frontend && npm install",
    "test:backend": "cd backend && npm test",
    "test:frontend": "cd frontend && npm test",
    "test:e2e": "cd frontend && npm run test:e2e",
    "validate": "node scripts/validate-baseline.mjs",
    "check:constitution": "node scripts/check-constitution.mjs",
    "create:test-event": "node scripts/create-manual-test-event.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
